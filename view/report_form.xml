<form string="AEAT 340 Report">
    <label name="company"/>
    <field name="company"/>
    <label name="period"/>
    <field name="period"/>
    <label name="fiscalyear"/>
    <field name="fiscalyear"/>
    <label name="fiscalyear_code"/>
    <field name="fiscalyear_code"/>
    <label name="company_vat"/>
    <field name="company_vat"/>
    <label name="previous_number"/>
    <field name="previous_number"/>
    <notebook colspan="4">
        <page string="General" id="general">
            <label name="contact_name"/>
            <field name="contact_name"/>
            <label name="contact_phone"/>
            <field name="contact_phone"/>
            <label name="representative_vat"/>
            <field name="representative_vat"/>
            <label name="company_vat"/>
            <field name="company_vat"/>
            <label name="type"/>
            <field name="type"/>
            <label name="support_type"/>
            <field name="support_type"/>
            <separator string="Totals" id="totals" colspan="4"/>
            <label name="record_count"/>
            <field name="record_count"/>
            <label name="taxable_total"/>
            <field name="taxable_total"/>
            <label name="sharetax_total"/>
            <field name="sharetax_total"/>
            <label name="total"/>
            <field name="total"/>
        </page>
        <page string="Issued" id="issued">
            <field name="issued_lines"/>
        </page>
        <page string="Received" id="received">
            <field name="received_lines"/>
        </page>
        <page string="Investment" id="investment">
            <field name="investment_lines"/>
        </page>
        <page string="Intracommunity" id="intracommunity">
            <field name="intracommunity_lines"/>
        </page>
    </notebook>
    <group id="state" colspan="2" col="6">
        <label name="state"/>
        <field name="state"/>
        <label name="calculation_date"/>
        <field name="calculation_date"/>
        <label name="file_"/>
        <field name="file_"/>
    </group>
    <group id="buttons" colspan="2">
        <button name="draft" string="Draft"/>
        <button name="calculate" string="Calculate"/>
        <button name="process" string="Process"/>
        <button name="cancel" string="Cancel"/>
    </group>
</form>
